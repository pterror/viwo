{
  "plugins": ["typescript", "import", "unicorn", "jest"],
  "rules": {
    "no-console": "off",
    "no-debugger": "error",
    "eqeqeq": "error",
    "import/no-cycle": "error"
  },
  "overrides": [
    {
      "files": ["*.test.ts", "*.test.tsx", "*.spec.ts", "*.spec.tsx"],
      "rules": {
        "no-magic-numbers": "off"
      }
    }
  ]
}
